{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.8" 
    },
    "commands": [
      "./build.sh"
    ]
  },
  "env": {
    "PORT": "8080"
  }
}


